{
  "version": 2,
  "buildCommand": "npm run build:prod",
  "outputDirectory": ".next", 
  "framework": "nextjs",
  "installCommand": "npm install",
  "env": {
    "NODE_ENV": "production",
    "SKIP_ENV_VALIDATION": "true",
    "DISABLE_ESLINT": "true",
    "TURBOPACK_TELEMETRY": "disabled"
  },
  "functions": {
    "app/api/**/*.js": {
      "maxDuration": 30
    }
  }
}